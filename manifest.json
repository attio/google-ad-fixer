{
    "manifest_version": 2,
   
    "name": "Ad Highlighter",
    "version": "0.2.0",
    "description": "Fixes Google Search results page to make ads more prominent",
   
    "content_scripts": [{
      "css": ["styles.css"],
      "matches": [
        "*://www.google.com/*",
        "*://www.google.ca/*",
        "*://www.google.co.uk/",
        "*://www.google.fr/",
        "*://www.google.de/"
      ],
      "run_at": "document_start"
    }]
   
  }